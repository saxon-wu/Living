{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "Api server based on nestjs",
  "author": "Saxon",
  "private": true,
  "license": "Apache",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/typeorm": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.14.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.1",
    "compression": "^1.7.4",
    "csurf": "^1.11.0",
    "dayjs": "^1.8.23",
    "ddos": "^0.2.1",
    "express-rate-limit": "^5.1.1",
    "file-type": "^16.5.4",
    "hashids": "^2.2.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "nestjs-typeorm-paginate": "^2.0.3",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "randomcolor": "^0.5.4",
    "read-chunk": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^2.1.0",
    "rxjs": "^6.5.4",
    "sharp": "^0.25.2",
    "svg-gradient": "^1.0.2",
    "text-to-svg": "^3.1.5",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.13.0",
    "@types/compression": "^1.7.0",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/faker": "^4.1.11",
    "@types/hashids": "^1.0.30",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.7",
    "@types/randomcolor": "^0.5.4",
    "@types/read-chunk": "^3.1.0",
    "@types/sharp": "^0.25.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
